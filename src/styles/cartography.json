{
  "version": 8,
  "glyphs": "https://demotiles.maplibre.org/font/{fontstack}/{range}.pbf",
  "sources": {
    "land-tiles": {
      "type": "vector",
      "url": "pmtiles://tiles/land.pmtiles"
    },
    "land-use-tiles": {
      "type": "vector",
      "url": "pmtiles://tiles/land_use.pmtiles"
    },
    "water-tiles": {
      "type": "vector",
      "url": "pmtiles://tiles/water.pmtiles"
    },
    "roads-tiles": {
      "type": "vector",
      "url": "pmtiles://tiles/roads.pmtiles"
    },
    "buildings-tiles": {
      "type": "vector",
      "url": "pmtiles://tiles/buildings.pmtiles"
    },
    "places-tiles": {
      "type": "vector",
      "url": "pmtiles://tiles/places.pmtiles"
    }
  },
  "layers": [
    {
      "id": "background",
      "type": "background",
      "layout": {},
      "paint": {
        "background-color": "#ffffff",
        "background-opacity": 0.25
      }
    },
    {
      "id": "land-use",
      "type": "fill",
      "source": "land-use-tiles",
      "source-layer": "layer",
      "layout": {},
      "paint": {
        "fill-color": [
          "match",
          ["get", "class"],
          "residential", "#e8f5e8",
          "commercial", "#ffeaa7",
          "retail", "#ffda79",
          "industrial", "#ddd6fe",
          "pedestrian", "#f1f5f9",
          "farmland", "#fde047",
          "farmyard", "#facc15",
          "orchard", "#84cc16",
          "vineyard", "#65a30d",
          "plant_nursery", "#a3e635",
          "park", "#a7f3d0",
          "recreation_ground", "#86efac",
          "playground", "#34d399",
          "pitch", "#10b981",
          "stadium", "#059669",
          "golf_course", "#16a34a",
          "fairway", "#22c55e",
          "green", "#15803d",
          "tee", "#166534",
          "bunker", "#d6d3d1",
          "water_hazard", "#3b82f6",
          "track", "#f97316",
          "camp_site", "#eab308",
          "theme_park", "#ec4899",
          "dog_park", "#84cc16",
          "forest", "#22c55e",
          "wilderness_area", "#15803d",
          "nature_reserve", "#15803d",
          "national_park", "#15803d",
          "state_park", "#15803d",
          "species_management_area", "#047857",
          "grass", "#65a30d",
          "meadow", "#84cc16",
          "garden", "#a3e635",
          "school", "#fbbf24",
          "university", "#f59e0b",
          "college", "#d97706",
          "hospital", "#ef4444",
          "religious", "#8b5cf6",
          "cemetery", "#71717a",
          "grave_yard", "#52525b",
          "construction", "#fb7185",
          "brownfield", "#a8a29e",
          "landfill", "#78716c",
          "quarry", "#57534e",
          "marina", "#0ea5e9",
          "military", "#dc2626",
          "village_green", "#bef264",
          "flowerbed", "#f472b6",
          "#00000000"
        ],
        "fill-opacity": 1
      }
    },
    {
      "id": "land",
      "type": "fill",
      "source": "land-tiles",
      "source-layer": "layer",
      "layout": {},
      "paint": {
        "fill-color": [
          "match",
          ["get", "subtype"],
          "forest", "#228B22",
          "shrub", "#9ACD32",
          "tree", "#006400",
          "wetland", "#20B2AA",
          "land", "#f0f0f0",
          "physical", "#d3d3d3",
          "grass", "#90EE90",
          "sand", "#F4A460",
          "rock", "#696969",
          "#00000000"
        ],
        "fill-opacity": 1
      }
    },
    {
      "id": "water-polygons",
      "type": "fill",
      "source": "water-tiles",
      "source-layer": "layer",
      "filter": ["==", ["geometry-type"], "Polygon"],
      "layout": {},
      "paint": {
        "fill-color": "#4a90e2",
        "fill-opacity": 1,
        "fill-outline-color": "#2980b9"
      }
    },
    {
      "id": "water-polygon-outlines",
      "type": "line",
      "source": "water-tiles",
      "source-layer": "layer",
      "filter": ["==", ["geometry-type"], "Polygon"],
      "layout": {
        "visibility": "visible"
      },
      "paint": {
        "line-color": "#2980b9",
        "line-width": 1,
        "line-opacity": 1
      }
    },
    {
      "id": "water-lines",
      "type": "line",
      "source": "water-tiles",
      "source-layer": "layer",
      "filter": ["==", ["geometry-type"], "LineString"],
      "layout": {},
      "paint": {
        "line-color": "#4a90e2",
        "line-width": [
          "match",
          ["get", "subtype"],
          "river", 3,
          "stream", 2,
          "canal", 2,
          1
        ],
        "line-opacity": 1
      }
    },
    {
      "id": "roads-solid",
      "type": "line",
      "source": "roads-tiles",
      "source-layer": "layer",
      "filter": ["in", ["get", "class"], ["literal", ["motorway", "trunk", "primary", "secondary", "tertiary", "residential"]]],
      "layout": {},
      "paint": {
        "line-color": [
          "match",
          ["get", "class"],
          "motorway", "#e74c3c",
          "trunk", "#e67e22",
          "primary", "#e74c3c",
          "secondary", "#f39c12",
          "tertiary", "#f1c40f",
          "residential", "#95a5a6",
          "#000"
        ],
        "line-width": [
          "interpolate",
          ["linear"],
          ["zoom"],
          10, [
            "match",
            ["get", "class"],
            "motorway", 1.5,
            "trunk", 1.2,
            "primary", 1,
            "secondary", 0.8,
            "tertiary", 0.6,
            "residential", 0.4,
            0.1
          ],
          12, [
            "match",
            ["get", "class"],
            "motorway", 3,
            "trunk", 2.5,
            "primary", 2,
            "secondary", 1.5,
            "tertiary", 1.2,
            "residential", 0.8,
            0.3
          ],
          14, [
            "match",
            ["get", "class"],
            "motorway", 5,
            "trunk", 4,
            "primary", 3.5,
            "secondary", 2.5,
            "tertiary", 2,
            "residential", 1.5,
            0.5
          ],
          16, [
            "match",
            ["get", "class"],
            "motorway", 8,
            "trunk", 6,
            "primary", 5,
            "secondary", 4,
            "tertiary", 3,
            "residential", 2,
            1
          ]
        ],
        "line-opacity": [
          "interpolate",
          ["linear"],
          ["zoom"],
          10, 0.6,
          12, 0.7,
          14, 0.8,
          16, 0.9
        ]
      }
    },
    {
      "id": "roads-dashed",
      "type": "line",
      "source": "roads-tiles",
      "source-layer": "layer",
      "filter": ["in", ["get", "class"], ["literal", ["service", "unclassified", "track", "path", "footway", "cycleway", "bridleway", "pedestrian", "steps"]]],
      "layout": {},
      "paint": {
        "line-color": [
          "match",
          ["get", "class"],
          "service", "#bdc3c7",
          "unclassified", "#95a5a6",
          "track", "#8b7355",
          "path", "#2ecc71",
          "footway", "#27ae60",
          "cycleway", "#3498db",
          "bridleway", "#9b59b6",
          "pedestrian", "#34495e",
          "steps", "#7f8c8d",
          "#000"
        ],
        "line-width": [
          "interpolate",
          ["linear"],
          ["zoom"],
          10, [
            "match",
            ["get", "class"],
            "service", 0.2,
            0.1
          ],
          12, [
            "match",
            ["get", "class"],
            "service", 0.5,
            0.3
          ],
          14, [
            "match",
            ["get", "class"],
            "service", 1,
            "unclassified", 1,
            "track", 0.8,
            "path", 0.8,
            "footway", 0.8,
            "cycleway", 0.8,
            "bridleway", 0.8,
            "pedestrian", 0.8,
            "steps", 0.5,
            0.5
          ],
          16, [
            "match",
            ["get", "class"],
            "service", 1.5,
            "unclassified", 1.5,
            "track", 1,
            "path", 1,
            "footway", 1,
            "cycleway", 1,
            "bridleway", 1,
            "pedestrian", 1,
            "steps", 0.5,
            1
          ]
        ],
        "line-opacity": [
          "interpolate",
          ["linear"],
          ["zoom"],
          10, 0.6,
          12, 0.7,
          14, 0.8,
          16, 0.9
        ],
        "line-dasharray": [2, 2]
      }
    },
    {
      "id": "places",
      "type": "circle",
      "source": "places-tiles",
      "source-layer": "layer",
      "minzoom": 15,
      "paint": {
        "circle-radius": 2,
        "circle-color": "#e74c3c",
        "circle-stroke-color": "#ffffff",
        "circle-stroke-width": 0.5
      }
    },
    {
      "id": "buildings",
      "type": "fill",
      "source": "buildings-tiles",
      "source-layer": "layer",
      "minzoom": 9,
      "layout": {},
      "paint": {
        "fill-color": "#8b8b8b",
        "fill-opacity": [
          "interpolate",
          ["linear"],
          ["zoom"],
          13, 0.3,
          14, 0.5,
          15, 0.7,
          16, 0.8
        ],
        "fill-outline-color": "#666"
      }
    },
    {
      "id": "building-outlines",
      "type": "line",
      "source": "buildings-tiles",
      "source-layer": "layer",
      "minzoom": 15,
      "layout": {},
      "paint": {
        "line-color": "#555",
        "line-width": 0.5,
        "line-opacity": 0.6
      }
    },
    {
      "id": "place-labels",
      "type": "symbol",
      "source": "places-tiles",
      "source-layer": "layer",
      "minzoom": 15,
      "layout": {
        "text-field": ["get", "name"],
        "text-font": ["Open Sans Regular", "Arial Unicode MS Regular"],
        "text-size": 12,
        "text-offset": [0, 1.5],
        "text-anchor": "top"
      },
      "paint": {
        "text-color": "#333333",
        "text-halo-color": "#ffffff",
        "text-halo-width": 1
      }
    }
  ]
}
